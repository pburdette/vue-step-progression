{
  "name": "vue-step-progression",
  "version": "1.0.11",
  "description": "A Vue component to help you build a multi-step form or other processes",
  "main": "dist/StepProgression.umd.js",
  "module": "dist/StepProgression.esm.js",
  "unpkg": "dist/StepProgression.min.js",
  "browser": {
    "./sfc": "src/StepProgression.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/StepProgression.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/StepProgression.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/StepProgression.min.js"
  },
  "dependencies": {},
  "devDependencies": {
    "minimist": "^1.2.0",
    "rollup": "^0.66.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.3.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "author": "Payton Burdette <paytonburd@gmail.com> (https://paytonburdette.com/)",
  "homepage": "https://github.com/beardedpayton/vue-step-progression#readme",
  "bugs": {
    "url": "https://github.com/beardedpayton/vue-step-progression/issues"
  },
  "keywords": [
    "vue",
    "multi step form",
    "component",
    "step progresssion",
    "steps"
  ]
}
